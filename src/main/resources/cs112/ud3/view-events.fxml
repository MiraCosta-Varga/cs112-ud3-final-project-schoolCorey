<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="389.0" prefWidth="343.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.ud3.controllers.ViewEvents">
   <children>
      <VBox alignment="BOTTOM_CENTER" prefHeight="400.0" prefWidth="343.0" spacing="20.0">
         <children>
            <Label text="Choose Event to View">
               <font>
                  <Font name="Century Schoolbook" size="24.0" />
               </font>
            </Label>
            <ComboBox fx:id="eventChoiceComboBox" onAction="#debugComboBoxAction" prefHeight="26.0" prefWidth="227.0" />
            <Pane prefHeight="318.0" prefWidth="343.0">
               <children>
                  <Button fx:id="backButton" layoutX="45.0" layoutY="245.0" mnemonicParsing="false" onAction="#onBackClick" text="Back" />
                  <Button fx:id="nextButton" layoutX="249.0" layoutY="245.0" mnemonicParsing="false" onAction="#onNextClick" text="Next" />
               </children>
            </Pane>
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
